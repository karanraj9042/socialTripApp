<p id="notice"><%= notice %></p>

<h1>Messages</h1>

<table>

  <tbody>
  <% @messages.each do |message| %>
	<div class="col-md-4">
		<div class="message">
			<h2 class="message-title"><%= message.title %></h2>
			<%= link_to "View Message", message, class: "btn-custom" %>
		</div>
	</div>
<% end %>
  </tbody>
</table>
